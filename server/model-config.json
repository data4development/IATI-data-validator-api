{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "iati-file": {
    "dataSource": "iati-filestore",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "download": true,
          "getFile": true,
          "getFiles": true
        }
      }
    }
  },
  "iati-dataset": {
    "dataSource": "iati-datasets",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "__get__completesvrl": true,
          "__count__completesvrl": true
        }
      }
    }
  },
  "iati-publisher": {
    "dataSource": "iati-publishers",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "__get__workspaces": true,
          "__count__workspaces": true,
          "prototype.__findById__workspaces": true,
          "__get__current": true,
          "__count__current": true,
          "__findOne__current": true
        }
      }
    }
  },
  "workspace": {
    "dataSource": "workspaces",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "__get__versions": true,
          "__count__versions": true,
          "prototype.__findById__versions": true
        }
      }
    }
  },
  "version": {
    "dataSource": "versions",
    "public": true
  },
  "iati-testfile": {
    "dataSource": "iati-testfilestore",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "download": true,
          "getFile": true
        }
      }
    }    
  },
  "iati-testdataset": {
    "dataSource": "iati-testdatasets",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "find": false,
          "count": false,
          "findOne": false,
          "create": true,
          "upload": true
        }
      }
    }
  }
}
